/* submenu */
.menu ul.sub-menu, .menu ul.children {
    display: none;
    margin-top: 60px;
    margin-left: 0px;
    position:absolute;
    
    left:auto;
    min-width:180px;
    background:#fff;
    background: rgba(255,255,255,0.98);
	opacity:1;
    z-index: 99999;
	-moz-box-shadow: 0 1px 10px rgba(0,0,0,0.15);
