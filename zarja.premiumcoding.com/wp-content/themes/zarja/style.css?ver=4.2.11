}
.menu-fixedmenu .menu ul.sub-menu {margin-top:50px;}
.menu ul.sub-menu li, .menu ul.children li {
    border-left: 0px;
    margin-right: 0;
    float:none;
    display:block;
    position: relative;
    text-align: left;
